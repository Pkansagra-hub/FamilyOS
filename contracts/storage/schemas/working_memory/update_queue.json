{
    "$schema": "https://json-schema.org/draft-07/schema#",
    "$id": "update_queue.schema.json",
    "title": "Working Memory Update Queue Storage",
    "type": "object",
    "required": [
        "queue_id",
        "pending_updates",
        "processing_order"
    ],
    "properties": {
        "queue_id": {
            "type": "string"
        },
        "pending_updates": {
            "type": "array",
            "items": {
                "type": "object"
            }
        },
        "processing_order": {
            "type": "string",
            "enum": [
                "fifo",
                "priority",
                "temporal"
            ]
        },
        "queue_length": {
            "type": "integer",
            "minimum": 0
        },
        "processing_rate": {
            "type": "number"
        }
    }
}
