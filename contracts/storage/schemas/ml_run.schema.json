{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://familyos.local/contracts/storage/ml_run.schema.json",
  "title": "ML Run",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "id",
    "ts",
    "space_id",
    "name",
    "params"
  ],
  "properties": {
    "id": {
      "$ref": "./common.schema.json#/$defs/ULID"
    },
    "ts": {
      "$ref": "./common.schema.json#/$defs/Timestamp"
    },
    "space_id": {
      "$ref": "./common.schema.json#/$defs/SpaceId"
    },
    "name": {
      "type": "string"
    },
    "params": {
      "type": "object",
      "additionalProperties": true
    },
    "status": {
      "type": "string",
      "enum": [
        "pending",
        "running",
        "complete",
        "failed"
      ]
    }
  }
}