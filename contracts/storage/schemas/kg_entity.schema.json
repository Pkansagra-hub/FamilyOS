{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://familyos.local/contracts/storage/kg_entity.schema.json",
  "title": "KG Entity",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "id",
    "space_id",
    "types",
    "labels",
    "created_ts"
  ],
  "properties": {
    "id": {
      "$ref": "./common.schema.json#/$defs/ULID"
    },
    "space_id": {
      "$ref": "./common.schema.json#/$defs/SpaceId"
    },
    "types": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "labels": {
      "type": "object",
      "additionalProperties": {
        "type": "string"
      }
    },
    "created_ts": {
      "$ref": "./common.schema.json#/$defs/Timestamp"
    },
    "props": {
      "type": "object",
      "additionalProperties": true
    }
  }
}