{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://familyos.local/schemas/events/hippocampus/episodic_memory_encoded.json",
    "title": "Episodic Memory Encoded Event",
    "description": "Event emitted when a new episodic memory is successfully encoded in the hippocampus, capturing the rich contextual and temporal information that characterizes episodic memories",
    "type": "object",
    "required": [
        "event_type",
        "timestamp",
        "hippocampus_id",
        "memory_id",
        "encoding_context",
        "episodic_content",
        "encoding_strength",
        "consolidation_priority"
    ],
    "properties": {
        "event_type": {
            "type": "string",
            "const": "episodic_memory_encoded",
            "description": "Event type identifier for episodic memory encoding"
        },
        "timestamp": {
            "type": "string",
            "format": "date-time",
            "description": "ISO 8601 timestamp when the episodic memory was encoded"
        },
        "hippocampus_id": {
            "type": "string",
            "pattern": "^hippocampus_[a-zA-Z0-9_-]+$",
            "description": "Unique identifier for the hippocampus instance performing encoding"
        },
        "memory_id": {
            "type": "string",
            "pattern": "^mem_[a-zA-Z0-9_-]+$",
            "description": "Unique identifier for the newly encoded episodic memory"
        },
        "encoding_context": {
            "type": "object",
            "required": [
                "spatial_context",
                "temporal_context",
                "emotional_context",
                "attention_state"
            ],
            "properties": {
                "spatial_context": {
                    "type": "object",
                    "required": [
                        "location_encoding",
                        "spatial_features"
                    ],
                    "properties": {
                        "location_encoding": {
                            "type": "string",
                            "description": "Encoded spatial location information"
                        },
                        "spatial_features": {
                            "type": "array",
                            "items": {
                                "type": "object",
                                "properties": {
                                    "feature_type": {
                                        "type": "string",
                                        "enum": [
                                            "place_cell",
                                            "grid_cell",
                                            "border_cell",
                                            "head_direction"
                                        ]
                                    },
                                    "activation_pattern": {
                                        "type": "string"
                                    },
                                    "spatial_resolution": {
                                        "type": "number",
                                        "minimum": 0
                                    }
                                }
                            },
                            "description": "Spatial feature activations during encoding"
                        },
                        "place_field_activity": {
                            "type": "array",
                            "items": {
                                "type": "object",
                                "properties": {
                                    "cell_id": {
                                        "type": "string"
                                    },
                                    "firing_rate": {
                                        "type": "number",
                                        "minimum": 0
                                    },
                                    "place_field_center": {
                                        "type": "array",
                                        "items": {
                                            "type": "number"
                                        }
                                    },
                                    "field_size": {
                                        "type": "number",
                                        "minimum": 0
                                    }
                                }
                            },
                            "description": "Place cell firing patterns during encoding"
                        }
                    },
                    "description": "Spatial context information for the episodic memory"
                },
                "temporal_context": {
                    "type": "object",
                    "required": [
                        "encoding_time",
                        "sequence_position"
                    ],
                    "properties": {
                        "encoding_time": {
                            "type": "string",
                            "format": "date-time",
                            "description": "Precise time when encoding occurred"
                        },
                        "sequence_position": {
                            "type": "integer",
                            "minimum": 0,
                            "description": "Position within temporal sequence of events"
                        },
                        "temporal_order_signal": {
                            "type": "string",
                            "description": "Neural signal encoding temporal order information"
                        },
                        "time_cell_activity": {
                            "type": "array",
                            "items": {
                                "type": "object",
                                "properties": {
                                    "cell_id": {
                                        "type": "string"
                                    },
                                    "temporal_field": {
                                        "type": "number",
                                        "minimum": 0
                                    },
                                    "firing_pattern": {
                                        "type": "string"
                                    },
                                    "sequence_coding": {
                                        "type": "string"
                                    }
                                }
                            },
                            "description": "Time cell activations encoding temporal context"
                        },
                        "theta_rhythm_phase": {
                            "type": "number",
                            "minimum": 0,
                            "maximum": 6.28318,
                            "description": "Theta rhythm phase during encoding (0-2Ï€ radians)"
                        }
                    },
                    "description": "Temporal context information for the episodic memory"
                },
                "emotional_context": {
                    "type": "object",
                    "required": [
                        "valence",
                        "arousal"
                    ],
                    "properties": {
                        "valence": {
                            "type": "number",
                            "minimum": -1,
                            "maximum": 1,
                            "description": "Emotional valence (-1 negative to +1 positive)"
                        },
                        "arousal": {
                            "type": "number",
                            "minimum": 0,
                            "maximum": 1,
                            "description": "Emotional arousal level (0 low to 1 high)"
                        },
                        "emotional_tags": {
                            "type": "array",
                            "items": {
                                "type": "string"
                            },
                            "description": "Categorical emotional labels associated with the memory"
                        },
                        "amygdala_modulation": {
                            "type": "object",
                            "properties": {
                                "modulation_strength": {
                                    "type": "number",
                                    "minimum": 0,
                                    "maximum": 1
                                },
                                "emotional_significance": {
                                    "type": "string",
                                    "enum": [
                                        "low",
                                        "moderate",
                                        "high",
                                        "critical"
                                    ]
                                },
                                "stress_hormone_level": {
                                    "type": "number",
                                    "minimum": 0
                                }
                            },
                            "description": "Amygdala modulation of hippocampal encoding"
                        }
                    },
                    "description": "Emotional context affecting memory encoding strength"
                },
                "attention_state": {
                    "type": "object",
                    "required": [
                        "attention_level",
                        "focus_type"
                    ],
                    "properties": {
                        "attention_level": {
                            "type": "number",
                            "minimum": 0,
                            "maximum": 1,
                            "description": "Level of attention during encoding (0-1)"
                        },
                        "focus_type": {
                            "type": "string",
                            "enum": [
                                "focused",
                                "divided",
                                "selective",
                                "sustained",
                                "executive"
                            ],
                            "description": "Type of attention deployed during encoding"
                        },
                        "attentional_networks": {
                            "type": "object",
                            "properties": {
                                "alerting": {
                                    "type": "number",
                                    "minimum": 0,
                                    "maximum": 1
                                },
                                "orienting": {
                                    "type": "number",
                                    "minimum": 0,
                                    "maximum": 1
                                },
                                "executive": {
                                    "type": "number",
                                    "minimum": 0,
                                    "maximum": 1
                                }
                            },
                            "description": "Attention network activation levels"
                        },
                        "working_memory_load": {
                            "type": "number",
                            "minimum": 0,
                            "maximum": 1,
                            "description": "Working memory load during encoding"
                        }
                    },
                    "description": "Attentional state during memory encoding"
                },
                "cognitive_state": {
                    "type": "object",
                    "properties": {
                        "consciousness_level": {
                            "type": "number",
                            "minimum": 0,
                            "maximum": 1
                        },
                        "metacognitive_awareness": {
                            "type": "number",
                            "minimum": 0,
                            "maximum": 1
                        },
                        "intentional_encoding": {
                            "type": "boolean"
                        },
                        "processing_mode": {
                            "type": "string",
                            "enum": [
                                "automatic",
                                "controlled",
                                "dual_process"
                            ]
                        }
                    },
                    "description": "Cognitive state during encoding"
                }
            },
            "description": "Rich contextual information captured during episodic encoding"
