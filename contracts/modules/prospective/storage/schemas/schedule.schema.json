{
  "$id": "https://familyos/contracts/prospective/schedule.schema.json",
  "$schema": "https://json-schema.org/draft-07/schema#",
  "type":"object",
  "required": ["schedule_id","due_at","space_id","band","status","created_ts"],
  "properties": {
    "schedule_id":{"type":"string"},
    "due_at":{"type":"string","format":"date-time"},
    "space_id":{"type":"string"},
    "band":{"type":"string","enum":["GREEN","AMBER","RED","BLACK"]},
    "status":{"type":"string","enum":["PENDING","CANCELLED","DONE"]},
    "payload":{"type":"object"},
    "created_ts":{"type":"string","format":"date-time"},
    "updated_ts":{"type":"string","format":"date-time"}
  }
}
